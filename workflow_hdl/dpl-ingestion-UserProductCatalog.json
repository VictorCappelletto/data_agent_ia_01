{
  "name": "dpl-ingestion-UserProductCatalog",
  "timeout_seconds": 0,
  "schedule": {
    "quartz_cron_expression": "<CRON_EXPRESSION_INGESTION_JOBS>",
    "timezone_id": "UTC",
    "pause_status": "<PAUSE_STATUS>"
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "UserAccounts",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Shared/Operations-Platform-dm-app/hdl/run_ingestion_ucc",
        "base_parameters": {
          "database": "UserProductCatalog",
          "table": "UserAccounts"
        },
        "source": "WORKSPACE"
      },
      "job_cluster_key": "CLUSTER_GENERAL_PURPOSE_MEDIUM",
      "max_retries": 1,
      "min_retry_interval_millis": 900000,
      "retry_on_timeout": false,
      "timeout_seconds": 5400,
      "health": {
        "rules": [
          {
            "metric": "RUN_DURATION_SECONDS",
            "op": "GREATER_THAN",
            "value": 3600
          }
        ]
      }
    }
  ],
  "tags": {
      "team": "hdl",
      "type": "ingestion"
    }
}